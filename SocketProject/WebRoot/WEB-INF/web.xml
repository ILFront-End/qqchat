<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginAction</servlet-name>
    <servlet-class>com.socket.action.LoginAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ResigerAction</servlet-name>
    <servlet-class>com.socket.action.ResigerAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Chataction</servlet-name>
    <servlet-class>com.socket.action.Chataction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SendAction</servlet-name>
    <servlet-class>com.socket.action.SendAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Checkuser</servlet-name>
    <servlet-class>com.socket.action.Checkuser</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>OnlineAction</servlet-name>
    <servlet-class>com.socket.action.OnlineAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GetImage</servlet-name>
    <servlet-class>com.socket.action.GetImage</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>LoginAction</servlet-name>
    <url-pattern>/servlet/LoginAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ResigerAction</servlet-name>
    <url-pattern>/servlet/ResigerAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Chataction</servlet-name>
    <url-pattern>/servlet/Chataction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SendAction</servlet-name>
    <url-pattern>/servlet/SendAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Checkuser</servlet-name>
    <url-pattern>/servlet/Checkuser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OnlineAction</servlet-name>
    <url-pattern>/servlet/OnlineAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetImage</servlet-name>
    <url-pattern>/servlet/GetImage</url-pattern>
  </servlet-mapping>

</web-app>